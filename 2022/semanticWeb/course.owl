<rdf:RDF xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xml:base="http://nfu.edu.csie.ccsd/">
<owl:Class rdf:ID="Person">
</owl:Class>
<owl:Class rdf:ID="Course">
</owl:Class>
<owl:Class rdf:ID="Teacher">
<rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>
<owl:Class rdf:ID="Student">
<rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>
<owl:Class rdf:ID="AI">
<rdfs:subClassOf rdf:resource="#Course"/>
</owl:Class>
<owl:Class rdf:ID="Algorithm">
<rdfs:subClassOf rdf:resource="#Course"/>
</owl:Class>
<owl:Class rdf:ID="Network">
<rdfs:subClassOf rdf:resource="#Course"/>
</owl:Class>
<owl:Class rdf:ID="ICdesign">
<rdfs:subClassOf rdf:resource="#Course"/>
</owl:Class>
<owl:Class rdf:ID="Exam">
</owl:Class>

<owl:SymmetricProperty rdf:ID="colleague">
<rdfs:domain rdf:resource="#Teacher"/>
<rdfs:range rdf:resource="#Teacher"/>
</owl:SymmetricProperty>
<owl:SymmetricProperty rdf:ID="classmate">
<rdfs:domain rdf:resource="#Student"/>
<rdfs:range rdf:resource="#Student"/>
</owl:SymmetricProperty>

<owl:ObjectProperty rdf:ID="isRelatedTo">
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
<rdfs:domain rdf:resource="#Course"/>
<rdfs:range rdf:resource="#Course"/>
</owl:ObjectProperty>


<owl:ObjectProperty rdf:ID="teach">
<rdfs:domain rdf:resource="#Teacher"/>
<rdfs:range rdf:resource="#Course"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:ID="isTaughtBy">
<owl:inverseOf rdf:resource="#teach"/>
<rdfs:domain rdf:resource="#Course"/>
<rdfs:range rdf:resource="#Teacher"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:ID="study">
<rdfs:domain rdf:resource="#Student"/>
<rdfs:range rdf:resource="#Course"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:ID="isStudiedBy">
<owl:inverseOf rdf:resource="#study"/>
<rdfs:domain rdf:resource="#Course"/>
<rdfs:range rdf:resource="#Student"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:ID="design">
<rdfs:domain rdf:resource="#Teacher"/>
<rdfs:range rdf:resource="#Exam"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:ID="isDesignedBy">
<owl:inverseOf rdf:resource="#design"/>
<rdfs:domain rdf:resource="#Exam"/>
<rdfs:range rdf:resource="#Teacher"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:ID="write">
<rdfs:domain rdf:resource="#Student"/>
<rdfs:range rdf:resource="#Exam"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:ID="isWrittenBy">
<owl:inverseOf rdf:resource="#write"/>
<rdfs:domain rdf:resource="#Exam"/>
<rdfs:range rdf:resource="#Student"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:ID="isTeacher">
<rdfs:domain rdf:resource="#Teacher"/>
<rdfs:range rdf:resource="#Student"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:ID="isStudent">
<owl:inverseOf rdf:resource="#isTeacher"/>
<rdfs:domain rdf:resource="#Student"/>
<rdfs:range rdf:resource="#Teacher"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:ID="courseName">
<rdfs:domain rdf:resource="#Course"/>
<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#String"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:ID="name">
<rdfs:domain rdf:resource="#Person"/>
<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#String"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:ID="courseLink">
<rdfs:domain rdf:resource="#Course"/>
<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#String"/>
</owl:DatatypeProperty>
</rdf:RDF>